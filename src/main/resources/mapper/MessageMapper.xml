<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.yt.nearresourceservice.dao.MessageDao">
    <insert id="addMessage" parameterType="com.yt.nearresourceservice.bean.Message">
        INSERT INTO message (userid,usercomment,replierid,resourceid)VALUES(#{userid},#{usercomment},#{replierid},#{resourceid});
    </insert>
</mapper>
